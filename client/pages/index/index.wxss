.classify{
    float:left;
    font-size: 30rpx;
    vertical-align: middle;
    color: black;
    line-height: 90rpx;
}

.body{
  width: 100%;
}
.create-tab{
  height:60rpx;
  width:100%;
  line-height:60rpx;
  font-size:35rpx;
  float:left;
  color:#704214;
  position:fixed;
bottom:50rpx;
left:250rpx;

}
.create-text{
  width: 250rpx;
  
}


/* 模态框样式-start */
.show-btn {
  margin-top: 100rpx;
  color: #22cc22;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  transition: all 300ms ease;
  width:100%;
overflow:hidden;
position:fixed;
top:0;
left:0;
z-index:9999;
background:#f9f9f9;
}

.modal-title {
  padding-top: 20rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}

.modal-content {
  padding: 50rpx 32rpx;
}

.modal-input {
  display: flex;
  background: #fff;
  border: 2rpx solid #ddd;
  border-radius: 4rpx;
  font-size: 28rpx;
}


.input {
  width: 100%;
  height: 82rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #333;
}

input-holder {
  color: #666;
  font-size: 28rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 50%;
  color: #ec5300;
  text-align: center;
}
/* 模态框样式-end */

.tips{
   width:100%;
  overflow:hidden;
  position:fixed;
  bottom:0%;
  left:0;
  z-index:9999;
  background:#f9f9f9;
  box-shadow: 1rpx 1rpx 1rpx #000
}

.tips view{
  width: 100%;
  text-align: center;
  height:80rpx;
border-bottom:1rpx solid #f2f2f2;
line-height:80rpx;

}
.list-select{
  background: #bbb;
}
.list-hover{
  background: #bbb;
}

/*加号*/
.icon-add-50{
    position: relative;
    box-sizing: border-box;
    border-radius: 50%;
    width:36rpx;
  height:36rpx;
  float:left;
  margin:15rpx 15rpx 10rpx 30rpx;
  vertical-align:middle;
  line-height:90rpx;
}
.icon-add-50:before{
    content: '';
    position: absolute;
    width: 16px;
    height: 1px;
    left: 50%;
    top: 50%;
    margin-left: -8px;
    margin-top: -1px;
    background-color: #704214;
}
.icon-add-50:after{
    content: '';
    position: absolute;
    width: 1px;
    height: 16px;
    left: 50%;
    top: 50%;
    margin-left: -1px;
    margin-top: -8px;
    background-color: #704214;
}

.sum{
  float:right;
margin:5px 20rpx 0 0;
border-radius:50%;
background:#cfcfcf;
width:60rpx;
height:60rpx;
line-height:60rpx;
text-align:center;
color:#fff;
font-size:56rpx;
vertical-align:medium;

}

.multi{
  color: yellow;
}
.top_flag{
  border:1px solid red;
border-radius:50%;
background:#f4000057;


}